<div class="page-header-my">
  <h1 class="text-center title">STREFA OSÓB PRYWATNYCH</h1>
</div>
<ol class="breadcrumb">
  <li><%= link_to 'Strona główna', home_index_path %></li>
  <li><%= link_to 'Szukam wolontariusza', look_for_volonteer_index_path %></li>
  <li class="active">Strefa osób prywatnych</li>
</ol>
  <nav>
      <div class="container-fluid">
          <div class"navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                <span class="sr-only"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
           </div>
      </div>
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
          <ul class="nav nav-justified nav-ind nav-all menu-text">
            <li class="active">
                <%= link_to 'Strona główna', home_index_path %>
            </li>
            <li class="active">
              <%= link_to "Zarejestruj się", new_user_registration_path %>
            </li>
            <li class="active">
              <%= link_to 'Wyszukaj wolontariusza', home_index_path %>
            </li>
          </ul>
      </div>
   </nav>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Surname</th>
      <th>Age</th>
      <th>City</th>
      <th>About</th>
      <th>Needs</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @individuals.each do |individual| %>
      <tr>
        <td><%= individual.name %></td>
        <td><%= individual.surname %></td>
        <td><%= individual.age %></td>
        <td><%= individual.city %></td>
        <td><%= individual.about %></td>
        <td><%= individual.needs %></td>
        <td><%= individual.user_id %></td>
        <td><%= link_to 'Show', individual %></td>
        <td><%= link_to 'Edit', edit_individual_path(individual) %></td>
        <td><%= link_to 'Destroy', individual, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

